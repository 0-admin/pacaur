#: pacaur:773
msgid "usage:  pacaur <operation> [options] [package(s)]"
msgstr "utilisation :  pacaur <operation> [options] [paquets]"

#: pacaur:774
msgid "operations:"
msgstr "opérations :"

#: pacaur:775
msgid " pacman extension"
msgstr " extensions de pacman"

#: pacaur:776
msgid "   -S, -Q           extend pacman operations to the AUR"
msgstr "   -S, -Q           opérations de pacman étendues à l'AUR"

#: pacaur:777
msgid " AUR only"
msgstr " AUR seulement"

#: pacaur:778
msgid "   -s, --search     search AUR repository for matching strings"
msgstr "   -s, --search     recherche de chaînes correspondantes dans l'AUR"

#: pacaur:779
msgid "   -i, --info       view package information -- pass twice for details"
msgstr ""
"   -i, --info       voir les informations du paquet -- passez deux fois pour "
"les détails"

#: pacaur:780
msgid ""
"   -d, --download   download target(s) -- pass twice to download AUR "
"dependencies"
msgstr ""
"   -d, --download   télécharger les cibles -- passez deux fois pour "
"télécharger les dépendances de l'AUR"

#: pacaur:781
msgid "   -m, --makepkg    download and make target(s)"
msgstr "   -m, --makepkg    télécharger et construire les cibles"

#: pacaur:782
msgid "   -y, --sync       download, make and install target(s)"
msgstr "   -y, --sync       télécharger, construire et installer les cibles"

#: pacaur:783
msgid "   -k, --check      check for AUR update(s)"
msgstr "   -k, --check      vérifier les mises à jour depuis l'AUR"

#: pacaur:784
msgid "   -u, --update     update AUR package(s)"
msgstr "   -u, --update     mettre à jour les paquets de l'AUR"

#: pacaur:785 pacaur:794
msgid " general"
msgstr " général"

#: pacaur:786
msgid "   -v, --version    display version information"
msgstr "   -v, --version    afficher les informations de version"

#: pacaur:787
msgid "   -h, --help       display help information"
msgstr "   -h, --help       afficher l'aide"

#: pacaur:788
msgid "   --fixbackend     quickly rebuild backend"
msgstr "   --fixbackend     reconstruire rapidement l'arrière-plan"

#: pacaur:790
msgid "options:"
msgstr "options :"

#: pacaur:791
msgid ""
" pacman extension - can be used with the -S, -Ss, -Si, -Sii, -Sw, -Su, -Qu, -"
"Sc, -Scc operations"
msgstr ""
" extensions de pacman - peuvent être utilisées avec les opérations -S, -Ss, -"
"Si, -Sii, -Sw, -Su, -Qu, -Sc, -Scc"

#: pacaur:792
msgid "   -a, --aur        only search, install or clean packages from the AUR"
msgstr ""
"   -a, --aur        ne rechercher, n'installer ou ne nettoyer que des "
"paquets de l'AUR"

#: pacaur:793
msgid ""
"   -r, --repo       only search, install or clean packages from the "
"repositories"
msgstr ""
"   -r, --repo       ne rechercher, n'installer ou ne nettoyer que des "
"paquets des dépôts"

#: pacaur:795
msgid "   -e, --edit       edit target(s) PKGBUILD and view install script"
msgstr ""
"   -e, --edit       modifier le PKGBUILD des cibles et en voir le script ."
"install"

#: pacaur:796
msgid ""
"   -c, --clean      clean target(s) build files -- can be combined with the -"
"m, -y, -u operations"
msgstr ""
"   -c, --clean      nettoyer les fichiers de construction des cibles -- peut "
"être combiné avec les opérations -m, -y, -u"

#: pacaur:797
msgid "   -q, --quiet      show less information for query and search"
msgstr ""
"   -q, --quiet      afficher moins d'informations avec -Q et pendant la "
"recherche"

#: pacaur:798
msgid "   --devel          consider AUR development packages upgrade"
msgstr ""
"   --devel          envisager les mises à jour de paquets de développement "
"de l'AUR"

#: pacaur:799
msgid ""
"   --ignore         ignore a package upgrade (can be used more than once)"
msgstr ""
"   --ignore         ignorer les mises à jour d'un paquet (peut être utilisé "
"plusieurs fois)"

#: pacaur:800
msgid "   --noconfirm      do not prompt for any confirmation"
msgstr "   --noconfirm      ne demander aucune confirmation"

#: pacaur:801
msgid "   --noedit         do not prompt to edit files"
msgstr "   --noedit         ne pas proposer la modification des fichiers"

#: pacaur:802
msgid "   --rebuild        always rebuild package(s)"
msgstr "   --rebuild        reconstruire systématiquement les paquets"

#: pacaur:811
msgid "Rebuilding ${colorW}cower${reset} backend..."
msgstr "Reconstruction de l'arrière-plan ${colorW}cower${reset}..."

